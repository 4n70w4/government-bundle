{
    "name": "avkluchko/government-bundle",
    "description": "Utilities for use with government classifiers",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Klyuchko",
            "email": "avkluchko@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/http-kernel": "^4.0"
    },
    "require-dev": {
        "symfony/test-pack": "^1.0",
        "symfony/phpunit-bridge": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "AVKluchko\\GovernmentBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AVKluchko\\GovernmentBundle\\Tests\\": "tests/"
        }
    }
}
